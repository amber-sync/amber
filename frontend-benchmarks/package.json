{
  "name": "amber-frontend-benchmarks",
  "version": "1.0.0",
  "description": "Frontend performance benchmarks for Amber backup application",
  "scripts": {
    "benchmark": "ts-node render_benchmarks.tsx",
    "benchmark:render": "ts-node render_benchmarks.tsx",
    "benchmark:interaction": "ts-node interaction_benchmarks.tsx",
    "benchmark:memory": "node --expose-gc memory_benchmarks.js",
    "benchmark:all": "npm run benchmark:render && npm run benchmark:interaction && npm run benchmark:memory",
    "benchmark:profile": "NODE_ENV=production webpack serve --open --mode production",
    "benchmark:ci": "npm run benchmark:all -- --reporter=json > benchmark-results.json"
  },
  "devDependencies": {
    "@testing-library/react": "^14.0.0",
    "@testing-library/jest-dom": "^6.1.4",
    "@types/react": "^18.2.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.0",
    "web-vitals": "^3.5.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
